{
  "name": "@verygoodplugins/mcp-automem",
  "version": "0.6.0",
  "description": "MCP server for AutoMem: AI memory storage and recall",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-automem": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && npm run postbuild",
    "postbuild": "chmod +x dist/index.js",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "tsx src/index.ts --help || true",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "modelcontextprotocol",
    "automem",
    "memory",
    "ai-memory",
    "falkordb",
    "qdrant"
  ],
  "author": "Very Good Plugins",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/verygoodplugins/mcp-automem.git"
  },
  "bugs": {
    "url": "https://github.com/verygoodplugins/mcp-automem/issues"
  },
  "homepage": "https://github.com/verygoodplugins/mcp-automem#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist/",
    "templates/",
    "env.example",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
